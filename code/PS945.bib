	pages = {53--66},
	file = {Full Text PDF:/Users/petererickson/Zotero/storage/DUVY4DR2/Mattes and Weeks - 2019 - Hawks, Doves, and Peace An Experimental Approach.pdf:application/pdf;Snapshot:/Users/petererickson/Zotero/storage/Y2J7RBF8/ajps.html:text/html}
